    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+V",
          "linux": "Ctrl+Shift+V",
          "windows": "Ctrl+Shift+V",
          "mac": "Command+Shift+V"
        }
      }
    },
    "manifest_version": 2
  }